{
  "name": "stylelint-sarif-formatter",
  "version": "1.0.6",
  "description": "A stylelint formatter for SARIF",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ."
  },
  "repository": {
    "url": "https://github.com/zhanwang626/stylelint-sarif-formatter.git",
    "type": "git"
  },
  "keywords": [
    "stylelint",
    "sarif",
    "formatter",
    "reporter",
    "report"
  ],
  "author": "Zhan Wang <wwzzvin626@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@microsoft/jest-sarif": "^1.0.0-beta.1",
    "eslint": "^7.24.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "release-it": "^14.11.6",
    "release-it-lerna-changelog": "^3.1.0"
  },
  "dependencies": {
    "tslib": "^2.2.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "12.22.6",
    "yarn": "1.22.11"
  }
}
